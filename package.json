{
  "name": "react-ssr-express-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsc && webpack --config webpack.prod.ts",
    "build:dev": "rimraf ./dist && tsc && webpack --config webpack.dev.ts",
    "start": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec node --inspect -r ts-node/register src/server.ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.7.1",
    "@types/pino": "^4.16.1",
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.7",
    "@types/uglifyjs-webpack-plugin": "^1.1.0",
    "@types/webpack": "^4.4.10",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.0",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.10.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "tsutils": "^3.0.0",
    "typescript": "^3.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "pino": "^5.2.0",
    "pino-pretty": "^2.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
