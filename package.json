{
  "name": "react-ssr-express-example",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsc && webpack --config webpack.prod.ts",
    "build:dev": "rimraf ./dist && tsc && webpack --config webpack.dev.ts",
    "start": "cross-env NODE_ENV=production node ./dist/server/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec node --inspect -r ts-node/register src/server.ts"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.4",
    "@types/pino": "^5.8.5",
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.2",
    "@types/webpack": "^4.4.24",
    "@types/webpack-merge": "^4.1.3",
    "@types/webpack-node-externals": "^1.6.3",
    "awesome-typescript-loader": "^5.2.1",
    "cross-env": "^5.2.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "source-map-loader": "^0.2.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "tsutils": "^3.8.0",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "express": "^4.16.4",
    "pino": "^5.11.1",
    "pino-pretty": "^2.5.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  }
}
